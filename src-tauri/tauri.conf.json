{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "passgen",
  "version": "0.1.0",
  "identifier": "com.passgen.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "passgen",
        "width": 640,
        "height": 860
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/Smile.ico"
    ]
  }
}

